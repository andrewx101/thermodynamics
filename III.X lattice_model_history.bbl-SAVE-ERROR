      \strng{authorfullhash}{cf44d40ebab9040b01739477709d066f}
      \strng{authorfullhashraw}{cf44d40ebab9040b01739477709d066f}
      \strng{nameabibnamehash}{cf44d40ebab9040b01739477709d066f}
      \strng{nameanamehash}{cf44d40ebab9040b01739477709d066f}
